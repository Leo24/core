{
    "name": "libre-ehr/core",
    "type": "library",
    "description": ":package_description",
    "keywords": [
        "LibreEHR",
        ":package_name"
    ],
    "homepage": "https://github.com/:vendor/:package_name",
    "license": "MIT",
    "authors": [
        {
            "name": "Ken Chapple",
            "email": "ken@mi-squared.com",
            "homepage": "http://mi-squared.com",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : "~5.5|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "LibreEHR\\Core\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LibreEHR\\Core\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
